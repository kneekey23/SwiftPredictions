{
  "name": "AmplifyPlugins",
  "version": "0.0.1",
  "summary": "Amazon Web Services Amplify for iOS.",
  "description": "AWS Amplify for iOS provides a declarative library for application development using cloud services",
  "homepage": "http://aws.amazon.com/mobile/sdk",
  "license": "Apache License, Version 2.0",
  "authors": {
    "Amazon Web Services": "amazonwebservices"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/aws-amplify/amplify-ios.git",
    "tag": "0.0.1"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "AWSAPICategoryPlugin",
      "source_files": "AmplifyPlugins/API/AWSAPICategoryPlugin/**/*.swift",
      "dependencies": {
        "AWSPluginsCore": [
          "0.0.1"
        ],
        "Starscream": [
          "~> 3.0.2"
        ]
      }
    },
    {
      "name": "AWSDataStoreCategoryPlugin",
      "source_files": "AmplifyPlugins/DataStore/AWSDataStoreCategoryPlugin/**/*.swift",
      "dependencies": {
        "AWSPluginsCore": [
          "0.0.1"
        ],
        "SQLite.swift": [
          "~> 0.12.0"
        ]
      }
    },
    {
      "name": "AWSPinpointAnalyticsPlugin",
      "source_files": "AmplifyPlugins/Analytics/AWSPinpointAnalyticsPlugin/**/*.swift",
      "dependencies": {
        "AWSPinpoint": [
          "~> 2.12.0"
        ],
        "AWSPluginsCore": [
          "0.0.1"
        ]
      }
    },
    {
      "name": "AWSPredictionsPlugin",
      "platforms": {
        "ios": "13.0"
      },
      "source_files": "AmplifyPlugins/Predictions/AWSPredictionsPlugin/**/*.swift",
      "dependencies": {
        "AWSComprehend": [
          "~> 2.12.0"
        ],
        "AWSPluginsCore": [
          "0.0.1"
        ],
        "AWSPolly": [
          "~> 2.12.0"
        ],
        "AWSRekognition": [
          "~> 2.12.0"
        ],
        "AWSTextract": [
          "~> 2.12.0"
        ],
        "AWSTranscribe": [
          "~> 2.12.0"
        ],
        "AWSTranslate": [
          "~> 2.12.0"
        ],
        "CoreMLPredictionsPlugin": [
          "0.0.1"
        ]
      }
    },
    {
      "name": "AWSS3StoragePlugin",
      "source_files": "AmplifyPlugins/Storage/AWSS3StoragePlugin/**/*.swift",
      "dependencies": {
        "AWSPluginsCore": [
          "0.0.1"
        ],
        "AWSS3": [
          "~> 2.12.0"
        ]
      }
    }
  ]
}
